                    <th class="th-shortText">Estado</th>
                    <th class="th-mediumText">Número Radicación Salida</th>
                    <th class="th-mediumText">Fecha Radicación Salida</th>
                    <th class="th-shortText">Acciones</th>

                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>


</div>



