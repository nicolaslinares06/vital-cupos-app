<partial name="Partials/_SeccionBotonesAyuda"></partial>

<div class="container">
    <div class="row col-12 mb-5">

        <h1>Gestion Precintos Nacionales y marquillas Coordinador</h1>

        <ul class="nav nav-tabs">
            <li class="nav-item" onclick="cargarTablaSolicitudesAnalistas('0')">
                <a class="nav-link active" href="#" data-bs-toggle="tab" id="tab-1">Asignar Analista</a>
            </li>
            <li class="nav-item" onclick="cargarTablaSolicitudesAnalistas('1')">
                <a class="nav-link" href="#" data-bs-toggle="tab" id="tab-2">Pendientes</a>
            </li>
            <li class="nav-item" onclick="cargarTablaSolicitudesAnalistas('2')">
